    "file-loader": "^0.10.0",
    "mustache": "^2.2.1",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "contentful": "^3.5.0",
    "flux": "^2.1.1",
